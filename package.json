{
  "name": "media_coordinates",
  "version": "1.0.0",
  "description": "AHJ Media task with geolocation, notifications and media recording",
  "homepage": "https://github.com/StanislavBalov/media_coordinates",
  "scripts": {
    "start": "webpack serve --config webpack.config.js --mode development --open",
    "build": "webpack --config webpack.config.js --mode production",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "test:debug": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --config=jest.config.mjs --watch",
    "test:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "set NODE_OPTIONS=--experimental-vm-modules && jest --coverage",
    "lint": "eslint src",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/preset-env": "^7.22.4",
    "@jest/globals": "^29.5.0",
    "@testing-library/jest-dom": "^6.6.3",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.42.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.4",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.31.0",
    "gh-pages": "^5.0.0",
    "html-webpack-plugin": "^5.5.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "style-loader": "^3.3.3",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
    "keywords": [
    "geolocation",
    "media",
    "webpack",
    "javascript"
  ],
  "author": "Stanislav Balov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/StanislavBalov/media_coordinates/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StanislavBalov/media_coordinates.git"
  }
}
